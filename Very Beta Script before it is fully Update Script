-- Graphic UI Implementation

-- Graphic Button Event Handler
LMG2L["Graphic_Button"].MouseButton1Click:Connect(function()
    local Content = LMG2L["Functions_Frame"]
    clearContentFrame(Content)
    
    -- Scrolling Frame
    local ScrollingFrame = Instance.new("ScrollingFrame")
    ScrollingFrame.Parent = Content
    ScrollingFrame.Size = UDim2.new(1, 0, 1, 0)
    ScrollingFrame.BackgroundTransparency = 1
    ScrollingFrame.ScrollBarThickness = 6
    ScrollingFrame.ScrollingDirection = Enum.ScrollingDirection.Y
    
    local UIListLayout = Instance.new("UIListLayout")
    UIListLayout.Parent = ScrollingFrame
    UIListLayout.Padding = UDim.new(0, 10)
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    
    task.wait(0.1)
    
    -- KATEGORI 1: LIGHTING & SHADOWS
    createCategoryHeader(ScrollingFrame, "ðŸŒž LIGHTING & SHADOWS")
    
    CreateGraphicSetting(ScrollingFrame, "Brightness", 1.0, 0.1, 0, 5, function(v)
        Lighting.Brightness = v
    end)
    
    CreateGraphicSetting(ScrollingFrame, "Shadow Quality", 0.5, 0.1, 0, 1, function(v)
        Lighting.ShadowSoftness = v
    end)
    
    CreateGraphicSetting(ScrollingFrame, "Ambient Light", 0.3, 0.1, 0, 1, function(v)
        Lighting.Ambient = Color3.fromRGB(v * 255, v * 255, v * 255)
    end)
    
    -- KATEGORI 2: WATER & REFLECTIONS
    createCategoryHeader(ScrollingFrame, "ðŸ’§ WATER & REFLECTIONS")
    
    if Terrain then
        CreateGraphicSetting(ScrollingFrame, "Water Reflection", 0.8, 0.1, 0, 1, function(v)
            Terrain.WaterReflectance = v
        end)
        
        CreateGraphicSetting(ScrollingFrame, "Wave Intensity", 0.5, 0.1, 0, 2, function(v)
            Terrain.WaterWaveSize = v
        end)
        
        CreateGraphicSetting(ScrollingFrame, "River Flow", 0.3, 0.1, 0, 2, function(v)
            Terrain.WaterWaveSpeed = v
        end)
        
        CreateGraphicSetting(ScrollingFrame, "Water Transparency", 0.7, 0.1, 0, 1, function(v)
            Terrain.WaterTransparency = v
        end)
    end
    
    -- KATEGORI 3: ENVIRONMENT
    createCategoryHeader(ScrollingFrame, "ðŸŒ¿ ENVIRONMENT")
    
    CreateGraphicSetting(ScrollingFrame, "Grass Sway", 0.5, 0.1, 0, 2, function(v)
        pcall(function()
            sethiddenproperty(workspace, "GlobalWind", Vector3.new(v * 3, 0, v * 1.5))
        end)
    end)
    
    -- KATEGORI 4: ATMOSPHERE
    createCategoryHeader(ScrollingFrame, "ðŸŒ«ï¸ ATMOSPHERE")
    
    CreateGraphicSetting(ScrollingFrame, "Fog Density", 0.1, 0.1, 0, 1, function(v)
        local atm = Lighting:FindFirstChildOfClass("Atmosphere")
        if not atm then
            atm = Instance.new("Atmosphere")
            atm.Parent = Lighting
        end
        atm.Density = v
        atm.Offset = 0.5
        atm.Color = Color3.fromRGB(200, 220, 255)
    end)
    
    -- KATEGORI 5: SPECIAL EFFECTS
    createCategoryHeader(ScrollingFrame, "âœ¨ SPECIAL EFFECTS")
    
    CreateToggleSetting(ScrollingFrame, "Water Splash Effects", false, setupWaterEffects)
    
    CreateToggleSetting(ScrollingFrame, "Wet Footprint Trails", false, function(enabled)
        print("Footprint Trails:", enabled and "ON" or "OFF")
    end)
    
    CreateToggleSetting(ScrollingFrame, "Fireflies in Grass", false, setupFireflies)
    
    CreateToggleSetting(ScrollingFrame, "Raindrop Ripples", false, function(enabled)
        print("Raindrop Ripples:", enabled and "ON" or "OFF")
    end)
    
    -- Update canvas size
    task.wait(0.2)
    ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y + 20)
end)

-- Initialize default values
if Terrain then
    Terrain.WaterReflectance = 0.8
    Terrain.WaterWaveSize = 0.5
    Terrain.WaterWaveSpeed = 0.3
    Terrain.WaterTransparency = 0.7
end

-- Setup default Atmosphere
if not Lighting:FindFirstChildOfClass("Atmosphere") then
    local atm = Instance.new("Atmosphere")
    atm.Density = 0.1
    atm.Offset = 0.5
    atm.Color = Color3.fromRGB(200, 220, 255)
    atm.Parent = Lighting
end

print("Part 6/8 Loaded - Copy Part 7...")

-- SHADERS COMPANY V0.5 - BAGIAN 7/8
-- Credits Module with RGB Animation

-- Credits animation variables
local creditsAnimations = {}
local rgbOffset = 0
local creditsActive = false

-- Fungsi animasi garis bergerak
local function createAnimatedLine(parent, position, size, direction)
    local lineContainer = Instance.new("Frame")
    lineContainer.Parent = parent
    lineContainer.Position = position
    lineContainer.Size = size
    lineContainer.BackgroundTransparency = 1
    lineContainer.ClipsDescendants = true
    
    local line = Instance.new("TextLabel")
    line.Name = "AnimatedLine"
    line.Parent = lineContainer
    line.Size = UDim2.new(2, 0, 1, 0)
    line.Position = direction == "right" and UDim2.new(-1, 0, 0, 0) or UDim2.new(0, 0, 0, 0)
    line.BackgroundTransparency = 1
    line.Text = "=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·=Ã·="
    line.TextColor3 = Color3.fromRGB(255, 255, 255)
    line.TextSize = 14
    line.TextXAlignment = Enum.TextXAlignment.Center
    line.FontFace = Font.new("rbxasset://fonts/families/HighwayGothic.json")
    
    return line
end

-- Fungsi teks RGB
local function createRGBText(parent, text, position, size)
    local textContainer = Instance.new("Frame")
    textContainer.Parent = parent
    textContainer.Position = position
    textContainer.Size = size
    textContainer.BackgroundTransparency = 1
    
    local shadow = Instance.new("TextLabel")
    shadow.Parent = textContainer
    shadow.Size = UDim2.new(1, 2, 1, 2)
    shadow.Position = UDim2.new(0, 1, 0, 1)
    shadow.BackgroundTransparency = 1
    shadow.Text = text
    shadow.TextColor3 = Color3.fromRGB(0, 0, 0)
    shadow.TextSize = 14
    shadow.FontFace = Font.new("rbxasset://fonts/families/HighwayGothic.json", Enum.FontWeight.Bold)
    shadow.TextXAlignment = Enum.TextXAlignment.Center
    
    local mainText = Instance.new("TextLabel")
    mainText.Parent = textContainer
    mainText.Size = UDim2.new(1, 0, 1, 0)
    mainText.Position = UDim2.new(0, 0, 0, 0)
    mainText.BackgroundTransparency = 1
    mainText.Text = text
    mainText.TextColor3 = Color3.fromRGB(255, 0, 0)
    mainText.TextSize = 14
    mainText.FontFace = Font.new("rbxasset://fonts/families/HighwayGothic.json", Enum.FontWeight.Bold)
    mainText.TextXAlignment = Enum.TextXAlignment.Center
    
    return mainText
end

-- Start credits animation
local function startCreditsAnimation(lineTop, lineBottom, rgbTexts)
    if creditsActive then return end
    creditsActive = true
    
    -- Cleanup
    for _, conn in pairs(creditsAnimations) do
        if conn then conn:Disconnect() end
    end
    creditsAnimations = {}
    
    -- Top line animation (right)
    local topConn = RunService.RenderStepped:Connect(function(delta)
        if lineTop and lineTop.Parent then
            local current = lineTop.Position
            local newX = current.X.Offset + (50 * delta)
            if newX > lineTop.Parent.AbsoluteSize.X then
                newX = -lineTop.AbsoluteSize.X
            end
            lineTop.Position = UDim2.new(current.X.Scale, newX, current.Y.Scale, current.Y.Offset)
        end
    end)
    table.insert(creditsAnimations, topConn)
    
    -- Bottom line animation (left)
    local bottomConn = RunService.RenderStepped:Connect(function(delta)
        if lineBottom and lineBottom.Parent then
            local current = lineBottom.Position
            local newX = current.X.Offset - (50 * delta)
            if newX < -lineBottom.AbsoluteSize.X then
                newX = lineBottom.Parent.AbsoluteSize.X
            end
            lineBottom.Position = UDim2.new(current.X.Scale, newX, current.Y.Scale, current.Y.Offset)
        end
    end)
    table.insert(creditsAnimations, bottomConn)
    
    -- RGB color animation
    local rgbConn = RunService.RenderStepped:Connect(function(delta)
        rgbOffset = (rgbOffset + delta * 2) % (math.pi * 2)
        for _, text in pairs(rgbTexts) do
            if text and text.Parent then
                local r = math.sin(rgbOffset) * 127 + 128
                local g = math.sin(rgbOffset + math.pi * 2/3) * 127 + 128
                local b = math.sin(rgbOffset + math.pi * 4/3) * 127 + 128
                text.TextColor3 = Color3.fromRGB(r, g, b)
            end
        end
    end)
    table.insert(creditsAnimations, rgbConn)
end

-- Stop credits animation
local function stopCreditsAnimation()
    creditsActive = false
    for _, conn in pairs(creditsAnimations) do
        if conn then conn:Disconnect() end
    end
    creditsAnimations = {}
end

print("Part 7/8 Loaded - Copy Part 8...")

-- SHADERS COMPANY V0.5 - BAGIAN 8/8
-- Credits UI & Final Integration

-- Credits UI Creation
local function createCreditsUI()
    local Content = LMG2L["Functions_Frame"]
    clearContentFrame(Content)
    
    local ScrollingFrame = Instance.new("ScrollingFrame")
    ScrollingFrame.Parent = Content
    ScrollingFrame.Size = UDim2.new(1, 0, 1, 0)
    ScrollingFrame.BackgroundTransparency = 1
    ScrollingFrame.ScrollBarThickness = 6
    ScrollingFrame.ScrollingDirection = Enum.ScrollingDirection.Y
    
    local UIListLayout = Instance.new("UIListLayout")
    UIListLayout.Parent = ScrollingFrame
    UIListLayout.Padding = UDim.new(0, 15)
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    
    -- Top animated line
    local topLineFrame = Instance.new("Frame")
    topLineFrame.Parent = ScrollingFrame
    topLineFrame.Size = UDim2.new(0.9, 0, 0, 25)
    topLineFrame.BackgroundTransparency = 1
    topLineFrame.LayoutOrder = 1
    
    local topLine = createAnimatedLine(topLineFrame, UDim2.new(0, 0, 0.5, 0), UDim2.new(1, 0, 0, 20), "right")
    
    -- Credits container
    local creditsContainer = Instance.new("Frame")
    creditsContainer.Parent = ScrollingFrame
    creditsContainer.Size = UDim2.new(0.9, 0, 0, 280)
    creditsContainer.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    creditsContainer.BorderSizePixel = 0
    creditsContainer.LayoutOrder = 2
    Instance.new("UICorner", creditsContainer).CornerRadius = UDim.new(0, 12)
    
    local creditsList = Instance.new("UIListLayout")
    creditsList.Parent = creditsContainer
    creditsList.Padding = UDim.new(0, 10)
    creditsList.SortOrder = Enum.SortOrder.LayoutOrder
    creditsList.HorizontalAlignment = Enum.HorizontalAlignment.Center
    
    local rgbTexts = {}
    
    -- Credits items
    local creditsData = {
        "ðŸ¦– CREDITS SCRIPT ðŸ¦–",
        "Script By: Dianozan ðŸ¦–",
        "Script Testing: Dianozan, Rezuko",
        "ðŸ‘¨ðŸ¼â€ðŸ’» Lua Code: Geminiâœ¨ & DeepseekðŸ‹",
        "ðŸ‘¨ðŸ¼â€ðŸ’¼ GUI Design: DianozanðŸ¦–",
        "ðŸžï¸ Graphic Ideas: DianozanðŸ¦– & DeepseekðŸ‹",
        "ðŸ› ï¸ Script Repair: DeepSeekðŸ‹",
        "SHADERS COMPANY V0.5"
    }
    
    for i, text in ipairs(creditsData) do
        local itemFrame = Instance.new("Frame")
        itemFrame.Parent = creditsContainer
        itemFrame.Size = UDim2.new(0.9, 0, 0, i == 1 and 40 or 30)
        itemFrame.BackgroundTransparency = 1
        itemFrame.LayoutOrder = i
        
        local itemText = createRGBText(itemFrame, text, UDim2.new(0, 0, 0, 0), UDim2.new(1, 0, 1, 0))
        itemText.TextSize = i == 1 and 16 or 14
        if i == #creditsData then
            itemText.TextColor3 = Color3.fromRGB(0, 255, 255)
            itemText.FontFace = Font.new("rbxasset://fonts/families/PressStart2P.json")
        else
            table.insert(rgbTexts, itemText)
        end
    end
    
    -- Bottom animated line
    local bottomLineFrame = Instance.new("Frame")
    bottomLineFrame.Parent = ScrollingFrame
    bottomLineFrame.Size = UDim2.new(0.9, 0, 0, 25)
    bottomLineFrame.BackgroundTransparency = 1
    bottomLineFrame.LayoutOrder = 3
    
    local bottomLine = createAnimatedLine(bottomLineFrame, UDim2.new(0, 0, 0.5, 0), UDim2.new(1, 0, 0, 20), "left")
    
    -- Start animation
    startCreditsAnimation(topLine, bottomLine, rgbTexts)
    
    task.wait(0.1)
    ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y + 20)
end

-- Credits button RGB animation
local function updateCreditsButtonColor()
    if LMG2L["Credits_Button"] and LMG2L["Credits_Button"].Parent then
        local t = tick()
        local r = math.sin(t * 2) * 127 + 128
        local g = math.sin(t * 2 + math.pi * 2/3) * 127 + 128
        local b = math.sin(t * 2 + math.pi * 4/3) * 127 + 128
        LMG2L["Credits_Button"].TextColor3 = Color3.fromRGB(r, g, b)
    end
end

local creditsButtonAnim = RunService.RenderStepped:Connect(updateCreditsButtonColor)

-- Credits button click event
LMG2L["Credits_Button"].MouseButton1Click:Connect(function()
    creditsButtonAnim:Disconnect()
    LMG2L["Credits_Button"].TextColor3 = Color3.fromRGB(255, 215, 0)
    createCreditsUI()
    task.wait(0.5)
    creditsButtonAnim = RunService.RenderStepped:Connect(updateCreditsButtonColor)
end)

-- Stop animation for other buttons
LMG2L["Graphic_Button"].MouseButton1Click:Connect(function()
    stopCreditsAnimation()
end)

LMG2L["Color_Button"].MouseButton1Click:Connect(function()
    stopCreditsAnimation()
end)

LMG2L["Weather_Button"].MouseButton1Click:Connect(function()
    stopCreditsAnimation()
end)

LMG2L["Library_Button"].MouseButton1Click:Connect(function()
    stopCreditsAnimation()
end)

